function cargarProducto()
{
    $.ajax({
        contex : document.body,
        url    : "Gestion/producto/producto.html"
           }).done(function(data)
                  {
                      document.getElementById("paginaInicial").innerHTML = data;
                  });
}
function cargarCliente()
{
    $.ajax({
        contex : document.body,
        url    : "Gestion/cliente/cliente.html"
           }).done(function(data)
                  {
                      $("#paginaInicial").html(data);
                  });
}
